{
    "name": "serverdensity/sd-api-wrapper",
    "type": "library",
    "description": "A php wrapper for the Server Density API v2",
    "homepage": "https://github.com/serverdensity/php-serverdensity-api",
    "keywords": ["serverdensity", "sd", "api", "php", "wrapper"],
    "license": "MIT",
    "authors": [
        {
            "name": "Server Density team",
            "homepage": "https://www.serverdensity.com",
            "email": "hello@serverdensity.com"
        }
    ],
    "require": {
        "php":             ">=5.3.2",
        "ext-curl":        "*",
        "guzzle/guzzle":   "~3.7",
        "guzzlehttp/guzzle": "~5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-0": { "serverdensity\\": "lib/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.4.x-dev"
        }
    }
}
